{
  "name": "@reside-eng/react-transition-group",
  "version": "0.0.0-development",
  "description": "A react component toolset for managing animations",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10.15.3"
  },
  "scripts": {
    "test": "yarn testonly",
    "testonly": "jest --verbose",
    "tdd": "jest --watch",
    "build": "babel src --out-dir lib --delete-dir-on-start && yarn build:dist && cp README.md LICENSE ./lib",
    "build:dist": "webpack --mode production",
    "lint": "eslint src test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-transition-group.git"
  },
  "keywords": [
    "react",
    "transition",
    "addons",
    "transition-group",
    "animation",
    "css",
    "transitions"
  ],
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/reactjs/react-transition-group/issues"
  },
  "homepage": "https://github.com/reactjs/react-transition-group#readme",
  "jest": {
    "testRegex": "-test\\.js",
    "setupFiles": [
      "./test/setup.js"
    ],
    "roots": [
      "<rootDir>/test"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "dependencies": {
    "dom-helpers": "^3.3.1",
    "loose-envify": "^1.4.0",
    "prop-types": "^15.6.2",
    "react-lifecycles-compat": "^3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@commitlint/cli": "8.3.5",
    "@side-eng/commitlint-config": "0.0.1",
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.18",
    "babel-preset-jason": "^6.0.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-jason": "^4.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^4.2.3",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "rimraf": "^2.6.1",
    "sinon": "^6.3.4",
    "webpack": "^4.19.1",
    "webpack-atoms": "^8.0.0",
    "webpack-cli": "^3.1.1"
  }
}
